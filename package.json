{
  "name": "docman",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "CI=false craco build",
    "test": "craco test",
    "test:css": "jest --config jest.stylelint.config.js",
    "eject": "react-scripts eject",
    "generate": "NODE_ENV='development' graphql-codegen --config codegen.cjs",
    "typecheck": "tsc --noEmit --incremental --project tsconfig.json",
    "lint-staged": "pnpm exec eslint --cache --ext .tsx,.ts && pnpm exec prettier --check",
    "lint": "pnpm exec eslint --cache --ext .tsx,.ts ./src",
    "lint:fix": "pnpm exec eslint --cache --ext .tsx,.ts ./src --fix",
    "prettier": "prettier --check ./src",
    "format": "pnpm exec prettier-eslint \"./src/**/*.{ts,tsx}\" --write",
    "prepare": "is-ci || husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "browserslist": {
    "production": [
      "last 10 Chrome versions",
      "last 10 Edge versions",
      "last 10 Firefox versions",
      "last 3 Android versions",
      "last 3 ChromeAndroid versions",
      "last 2 Safari major versions",
      "last 2 iOS major versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Edge versions",
      "last 1 Firefox versions",
      "last 1 Safari major versions"
    ]
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "volta": {
    "node": "16.15.0"
  },
