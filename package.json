{
  "name": "doc-man-ui",
  "version": "0.0.1",
  "description": "Document manufactory",
  "private": true,
  "keywords": [
    "document"
  ],
  "browserslist": {
    "production": [
      "last 10 Chrome versions",
      "last 10 Edge versions",
      "last 10 Firefox versions",
      "last 3 Android versions",
      "last 3 ChromeAndroid versions",
      "last 2 Safari major versions",
      "last 2 iOS major versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Edge versions",
      "last 1 Firefox versions",
      "last 1 Safari major versions"
    ]
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6.30.0"
  },
  "main": "main.tsx",
  "scripts": {
    "start": "vite",
    "prepare": "is-ci || husky install",
    "preinstall": "npx only-allow pnpm",
    "build": "tsc --noEmit && pnpm exec -- vite build",
    "preview": "vite preview --port 8888",
    "build:dev": "vite build --mode development",
    "test": "jest",
    "lint": "eslint --cache --ext .tsx,.ts ./src",
    "lint:fix": "pnpm prettier:fix && pnpm lint --fix",
    "prettier:check": "prettier --check ./",
    "prettier:fix": "prettier --write ./"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^2.0.0-next.0",
    "@chakra-ui/system": "^1.12",
    "@chakra-ui/theme-tools": "^1.3.6",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@types/node": "^17.0.24",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "ahooks": "^3.3.0",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.0",
    "framer-motion": "^6.3.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "immer": "^9.0.12",
    "jotai": "^1.6.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^3.3.2",
    "qs": "^6.10.3",
    "rc-upload": "^4.3.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^4.0.0-beta.3",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.2",
    "store2": "^2.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@commitlint/config-conventional": "^16.2.1",
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/eslint-plugin": "^11.7.0",
    "@rollup/plugin-inject": "^4.0.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-react": "^1.3.1",
    "commitlint": "^16.2.3",
    "commitlint-cli": "^1.1.3",
    "eslint": "^7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "git-describe": "^4.1.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.4",
    "vite-plugin-html": "^3.2.0",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0",
        "@types/react": "^17.0.0 || ^18.0.0",
        "@types/react-dom": "^17.0.0 || ^18.0.0"
      }
    }
  }
}
